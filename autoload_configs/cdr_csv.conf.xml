<configuration name="cdr_csv.conf" description="CDR CSV Format">
  <settings>
    <!-- 'cdr-csv' will always be appended to log-base -->
    <!--<param name="log-base" value="/var/log"/>-->
    <param name="default-template" value="jerasoft"/>
    <!-- This is like the info app but after the call is hung up -->
    <!--<param name="debug" value="true"/>-->
    <param name="rotate-on-hup" value="true"/>
    <!-- may be a b or ab -->
    <param name="legs" value="ab"/>
	<!-- Only log in Master.csv -->
    <param name="master-file-only" value="true"/> 
  </settings>
  <templates>
    <template name="jerasoft">"${coreuuid}","${uuid}","${network_addr}","${sip_to_host}","${sip_auth_username}","${caller_id_number}","${destination_number}","${sip_to_uri}","${start_stamp}","${answer_stamp}","${end_stamp}","${billsec}","${hangup_cause}","${progresssec}","${read_codec}","${write_codec}","${call-origin}","${touser}"</template>
  </templates>
</configuration>

